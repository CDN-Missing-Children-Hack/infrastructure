{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "CognitiveTextName": {
      "type": "string",
      "defaultValue": "CognitiveText",
      "metadata": {
        "description": "Name of the CognitiveText service"
      }
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.CognitiveServices/accounts",
      "sku": {
        "name": "F0"
      },
      "kind": "TextAnalytics",
      "name": "[parameters('CognitiveTextName')]",
      "apiVersion": "2016-02-01-preview",
      "location": "westus",
      "properties": {},
      "dependsOn": []
    }
  ],
  "outputs": {}
}