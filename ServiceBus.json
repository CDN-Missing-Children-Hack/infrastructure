{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {},
    "variables": {
        "serviceBusNamespaceName": "mcscservicebus",
        "serviceBusQueueName": "queue1"
    },
    "resources": [
        {
            "apiVersion": "2015-08-01",
            "name": "mcscservicebus",
            "type": "Microsoft.ServiceBus/namespaces",
            "location": "[resourceGroup().location]",
            "properties": {
                "messagingSku": "1"
            },
            "resources": [
                {
                    "apiVersion": "2015-08-01",
                    "name": "[variables('serviceBusQueueName')]",
                    "type": "Queues",
                    "dependsOn": [
                        "[concat('Microsoft.ServiceBus/namespaces/','mcscservicebus')]"
                    ],
                    "properties": {
                        "path": "[variables('serviceBusQueueName')]"
                    }
                }
            ]
        }
    ],
    "outputs": {}
}