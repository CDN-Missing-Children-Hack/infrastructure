{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "serviceBusNamespaceName": {
            "type": "string",
            "defaultValue": "GEN-UNIQUE",
            "metadata": {
                "description": "Name of the Service Bus Namespace"
            }
        },
        "serviceBusQueueName1": {
            "type": "string",
            "defaultValue": "Queue1",
            "metadata": {
                "description": "Name of the Service Bus Queue"
            }
        },
        "serviceBusQueueName2": {
            "type": "string",
            "defaultValue": "Queue2",
            "metadata": {
                "description": "Name of the Service Bus Queue"
            }
        },
        "components_mcsc_webapp_name": {
            "defaultValue": "mcsc-webapp",
            "type": "String"
        },
        "serverfarms_mcscserviceplan_name": {
            "defaultValue": "mcscserviceplan",
            "type": "String"
        },
        "sites_MCSC_webapp_name": {
            "defaultValue": "MCSC-webapp",
            "type": "String"
        }
    },
    "variables": {
        "sbVersion": "2015-08-01"
    },
    "resources": [
        {
            "apiVersion": "[variables('sbVersion')]",
            "name": "[parameters('serviceBusNamespaceName')]",
            "type": "Microsoft.ServiceBus/namespaces",
            "location": "[resourceGroup().location]",
            "properties": {},
            "resources": [
                {
                    "apiVersion": "[variables('sbVersion')]",
                    "name": "[parameters('serviceBusQueueName1')]",
                    "type": "Queues",
                    "dependsOn": [
                        "[concat('Microsoft.ServiceBus/namespaces/', parameters('serviceBusNamespaceName'))]"
                    ],
                    "properties": {},
                    "resources": []
                },
                {
                    "apiVersion": "[variables('sbVersion')]",
                    "name": "[parameters('serviceBusQueueName2')]",
                    "type": "Queues",
                    "dependsOn": [
                        "[concat('Microsoft.ServiceBus/namespaces/', parameters('serviceBusNamespaceName'))]"
                    ],
                    "properties": {},
                    "resources": []
                }
            ]
        },
        {
            "type": "microsoft.insights/components",
            "kind": "web",
            "name": "[parameters('components_mcsc_webapp_name')]",
            "apiVersion": "2014-04-01",
            "location": "Central US",
            "tags": {
                "hidden-link:/subscriptions/54a522b6-6cd7-4325-b4e6-566f9d921835/resourceGroups/MCSC-Dev/providers/Microsoft.Web/sites/MCSC-webapp": "Resource"
            },
            "properties": {
                "ApplicationId": "[parameters('components_mcsc_webapp_name')]"
            },
            "resources": [],
            "dependsOn": []
        },
        {
            "type": "Microsoft.Web/serverfarms",
            "sku": {
                "name": "S1",
                "tier": "Standard",
                "size": "S1",
                "family": "S",
                "capacity": 1
            },
            "name": "[parameters('serverfarms_mcscserviceplan_name')]",
            "apiVersion": "2015-08-01",
            "location": "West US",
            "properties": {
                "name": "[parameters('serverfarms_mcscserviceplan_name')]",
                "numberOfWorkers": 1
            },
            "resources": [],
            "dependsOn": []
        },
        {
            "type": "Microsoft.Web/sites",
            "name": "[parameters('sites_MCSC_webapp_name')]",
            "apiVersion": "2015-08-01",
            "location": "West US",
            "tags": {
                "hidden-related:/subscriptions/54a522b6-6cd7-4325-b4e6-566f9d921835/resourcegroups/MCSC-Dev/providers/Microsoft.Web/serverfarms/mcscserviceplan": "empty"
            },
            "properties": {
                "name": "[parameters('sites_MCSC_webapp_name')]",
                "hostNames": [
                    "mcsc-webapp.azurewebsites.net"
                ],
                "enabledHostNames": [
                    "mcsc-webapp.azurewebsites.net",
                    "mcsc-webapp.scm.azurewebsites.net"
                ],
                "hostNameSslStates": [
                    {
                        "name": "[concat(parameters('sites_MCSC_webapp_name'),'mcsc-webapp.azurewebsites.net')]",
                        "sslState": 0,
                        "thumbprint": null,
                        "ipBasedSslState": 0
                    },
                    {
                        "name": "[concat(parameters('sites_MCSC_webapp_name'),'mcsc-webapp.scm.azurewebsites.net')]",
                        "sslState": 0,
                        "thumbprint": null,
                        "ipBasedSslState": 0
                    }
                ],
                "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', parameters('serverfarms_mcscserviceplan_name'))]"
            },
            "resources": [],
            "dependsOn": [
                "[resourceId('Microsoft.Web/serverfarms', parameters('serverfarms_mcscserviceplan_name'))]"
            ]
        }
    ],
    "outputs": {}
}