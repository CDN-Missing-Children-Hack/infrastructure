{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "components_mcsc_webapp_name": {
            "defaultValue": "mcsc-ClientAPI",
            "type": "String",
        },
        "components_mcsc_webapp2_name": {
            "defaultValue": "mcsc-authorization",
            "type": "String",
        },
        "serverfarms_mcscserviceplan_name": {
            "defaultValue": "mcscserviceplan",
            "type": "String",
        },
    },
    "variables": {
        "sbVersion": "2015-08-01"
    },
    "resources": [
        {
            "type": "microsoft.insights/components",
            "kind": "web",
            "name": "[parameters('components_mcsc_webapp_name')]",
            "apiVersion": "2014-04-01",
            "location": "[resourceGroup().location]",
            "tags": {
                "hidden-link:/subscriptions/54a522b6-6cd7-4325-b4e6-566f9d921835/resourceGroups/MCSC-Dev/providers/Microsoft.Web/sites/MCSC-webapp": "Resource"
            },
            "properties": {
                "ApplicationId": "[parameters('components_mcsc_webapp_name')]"
            },
            "resources": [],
            "dependsOn": []
        },
        {
            "type": "microsoft.insights/components",
            "kind": "web",
            "name": "[parameters('components_mcsc_webapp2_name')]",
            "apiVersion": "2014-04-01",
            "location": "[resourceGroup().location]",
            "tags": {
                "hidden-link:/subscriptions/54a522b6-6cd7-4325-b4e6-566f9d921835/resourceGroups/MCSC-Dev/providers/Microsoft.Web/sites/MCSC-webapp": "Resource"
            },
            "properties": {
                "ApplicationId": "[parameters('components_mcsc_webapp2_name')]"
            },
            "resources": [],
            "dependsOn": []
        },
        {
            "type": "Microsoft.Web/serverfarms",
            "sku": {
                "name": "S1",
                "tier": "Standard",
                "size": "S1",
                "family": "S",
                "capacity": 1
            },
            "name": "[parameters('serverfarms_mcscserviceplan_name')]",
            "apiVersion": "2015-08-01",
            "location": "[resourceGroup().location]",
            "properties": {
                "name": "[parameters('serverfarms_mcscserviceplan_name')]",
                "numberOfWorkers": 1
            },
            "resources": [],
            "dependsOn": []
        },
        {
            "type": "Microsoft.Web/sites",
            "name": "[parameters('components_mcsc_webapp_name')]",
            "apiVersion": "2015-08-01",
            "location": "[resourceGroup().location]",
            "tags": {
                "hidden-related:/subscriptions/54a522b6-6cd7-4325-b4e6-566f9d921835/resourcegroups/MCSC-Dev/providers/Microsoft.Web/serverfarms/mcscserviceplan": "empty"
            },
            "properties": {
                "name": "[parameters('components_mcsc_webapp_name')]",
                "hostNames": [
                    "mcsc-webapp.azurewebsites.net"
                ],
                "enabledHostNames": [
                    "[concat(parameters('components_mcsc_webapp_name'),'.azurewebsites.net')]",
                    "[concat(parameters('components_mcsc_webapp_name'),'.scm.azurewebsites.net')]"
                ],
                "hostNameSslStates": [
                    {
                        "name": "[concat(parameters('components_mcsc_webapp_name'),'mcsc-webapp.azurewebsites.net')]",
                        "sslState": 0,
                        "thumbprint": null,
                        "ipBasedSslState": 0
                    },
                    {
                        "name": "[concat(parameters('components_mcsc_webapp_name'),'mcsc-webapp.scm.azurewebsites.net')]",
                        "sslState": 0,
                        "thumbprint": null,
                        "ipBasedSslState": 0
                    }
                ],
                "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', parameters('serverfarms_mcscserviceplan_name'))]"
            },
            "resources": [],
            "dependsOn": [
                "[resourceId('Microsoft.Web/serverfarms', parameters('serverfarms_mcscserviceplan_name'))]"
            ]
        },
        {
            "type": "Microsoft.Web/sites",
            "name": "[parameters('components_mcsc_webapp2_name')]",
            "apiVersion": "2015-08-01",
            "location": "[resourceGroup().location]",
            "tags": {
                "hidden-related:/subscriptions/54a522b6-6cd7-4325-b4e6-566f9d921835/resourcegroups/MCSC-Dev/providers/Microsoft.Web/serverfarms/mcscserviceplan": "empty"
            },
            "properties": {
                "name": "[parameters('components_mcsc_webapp2_name')]",
                "hostNames": [
                    "mcsc-webapp.azurewebsites.net"
                ],
                "enabledHostNames": [
                    "[concat(parameters('components_mcsc_webapp2_name'),'.azurewebsites.net')]",
                    "[concat(parameters('components_mcsc_webapp2_name'),'.scm.azurewebsites.net')]"
                ],
                "hostNameSslStates": [
                    {
                        "name": "[concat(parameters('components_mcsc_webapp2_name'),'mcsc-webapp.azurewebsites.net')]",
                        "sslState": 0,
                        "thumbprint": null,
                        "ipBasedSslState": 0
                    },
                    {
                        "name": "[concat(parameters('components_mcsc_webapp2_name'),'mcsc-webapp.scm.azurewebsites.net')]",
                        "sslState": 0,
                        "thumbprint": null,
                        "ipBasedSslState": 0
                    }
                ],
                "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', parameters('serverfarms_mcscserviceplan_name'))]"
            },
            "resources": [],
            "dependsOn": [
                "[resourceId('Microsoft.Web/serverfarms', parameters('serverfarms_mcscserviceplan_name'))]"
            ]
        }
    ],
    "outputs": {}
}